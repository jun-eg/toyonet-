{
  "name": "toyo-net-pluses",
  "displayName": "Toyo net pluses",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "jun",
  "scripts": {
    "notios": "notios dev",
    "dev": "run-p dev:*",
    "dev:plasmo": "plasmo dev",
    "dev:hcm": "hcm '**/*.module.css' -w --ignore-path .gitignore",
    "build": "plasmo build",
    "package": "plasmo package",
    "hcm": "hcm '**/*.module.css'",
    "typecheck": "pnpm hcm && tsc --noEmit"
  },
  "dependencies": {
    "less": "^4.2.0",
    "plasmo": "0.86.3",
    "postcss": "^8.4.38",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.77.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "happy-css-modules": "^3.0.1",
    "notios": "^0.4.1",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ]
  }
}
